variable "db2_database" {
  description = "The name of the DB2 database"
  default     = "PROXIMO"
}

variable "db2_user" {
  description = "The username for the DB2 database"
  default     = "db2admin"
}

variable "db2_password" {
  description = "The password for the DB2 database"
  default     = "newpassword"
}
